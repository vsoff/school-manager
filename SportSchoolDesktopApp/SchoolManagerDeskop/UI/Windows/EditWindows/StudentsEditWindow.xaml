<Window x:Class="SchoolManagerDeskop.UI.Windows.EditWindows.StudentsEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:editors="clr-namespace:SchoolManagerDeskop.UI.Views.EditWindows"
        mc:Ignorable="d"
        Title="Окно редактирования учеников"
        Height="450"
        Width="800"
        MinHeight="450"
        MinWidth="800">
    <Window.Resources>
        <ResourceDictionary Source="EditWindowsResources.xaml" />
    </Window.Resources>
    <Page Template="{StaticResource EditWindowTemplate}">
        <editors:StudentEditorView DataContext="{Binding EditorViewModel}"/>
    </Page>
</Window>
